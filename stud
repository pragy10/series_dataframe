import pandas as pd
import numpy as np
import string

d1={'sid':[101,102,103,104,105], 
    'name':['jen','ken','ram','anil','kapil'],
    'gender':['m','f','m','m','f'],
    'stream':['com','eco','phy','che','mat'],
    'marks':[98,97,92,95,92],
    'city':['chennai','delhi','mumbai','lucknow','mysore']}
